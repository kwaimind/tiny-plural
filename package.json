{
  "version": "1.1.4",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kwaimind/tinyplural.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "test:coverage": "vitest --coverage",
    "lint": "eslint",
    "size": "yarn build && size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "name": "tinyplural",
  "author": "Daniel Reed",
  "module": "dist/tinyplural.esm.js",
  "size-limit": [
    {
      "path": "dist/tinyplural.cjs.production.min.js",
      "limit": "1 KB"
    },
    {
      "path": "dist/tinyplural.esm.js",
      "limit": "1 KB"
    }
  ],
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@size-limit/preset-small-lib": "^5.0.1",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "prettier": "3.3.3",
    "size-limit": "^5.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vitest": "^2.1.3"
  }
}
